citHeader("To cite package 'AGread' in publications use:")

citEntry(entry = "Misc",
  title        = "AGread: Read Data Files from ActiGraph Monitors",
  author       = personList(as.person("Paul R. Hibbing")),
  year         = "2018",
  url          = "https://github.com/paulhibbing/AGread/",

  textVersion  =
  paste("Paul R. Hibbing (2018).",
        "AGread: Read Data Files from ActiGraph Monitors.",
        "URL https://github.com/paulhibbing/AGread/.")
)